        "@reduxjs/toolkit": "^1.9.5",
        "axios": "^1.4.0",
        "chart.js": "^4.3.0",
        "dayjs": "^1.11.9",
        "firebase": "^10.0.0",
        "i18next": "^23.2.11",
        "i18next-browser-languagedetector": "^7.1.0",
